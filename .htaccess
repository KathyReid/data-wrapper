# rewrites URL to datawrapper.de
RewriteEngine On
RewriteBase /

RewriteCond %{HTTP_HOST} !^localhost$
RewriteCond %{HTTP_HOST} !^datawrapper\.de$ [NC]
RewriteRule ^(.*)$ http://datawrapper.de/$1 [R=301, L]

# password protection allowing directory and file access
#AuthUserFile c:/wamp/www/Data-Story/.htpasswd
AuthUserFile /data/local/depjrkhynun/.htpasswd
AuthType Basic
AuthName "Restricted"
Require valid-user 